                  "// Try to parse JSON response\r",
                  "let jsonData;\r",
                  "try {\r",
                  "    jsonData = pm.response.json();\r",
                  "} catch (e) {\r",
                  "    pm.test(\"Response body is valid JSON\", function () {\r",
                  "        throw new Error(\"Response is not valid JSON\");\r",
                  "    });\r",
                  "}\r",
                  "\r",
                  "// Adjust this path if your API wraps the token differently\r",
                  "const token = jsonData.access_token;\r",
                  "\r",
                  "pm.test(\"Login response contains access_token\", function () {\r",
